<!-- Repair View Modal -->
<div class="modal fade" id="RepairView" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="repairModalLabel">Repair</h5>
                <button type="button" id="closeBtn" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form>
                    <table class="admin" style="width: 100%">
                        <thead>
                            
                            <tr class="text-nowrap border-bottom">
                                <th class="w-10">#</th>
                                <th class="w-15">Start Date:</th>
                                <th class="w-15">Target Date:</th>
                                <th class="w-25">Type:</th>
                                <th class="w-25">Progress:</th>
                                <th class="w-10">&nbsp;</th>

                            </tr>
                        </thead>
                        <tbody>
